    <![endif]-->

</head>
<body leftmargin="0" marginwidth="0" topmargin="0" marginheight="0" offset="0" style="min-width: 100%;margin: 0;padding: 0;-webkit-text-size-adjust: 100%;-ms-text-size-adjust: 100%;background-color: #fff;width: 100% !important;">

    {% for block in blocks %}
        {{ block|minify|raw }}
    {% endfor %}

</body>
</html>